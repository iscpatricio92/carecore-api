
NODE_ENV=development
PORT=3000
APP_NAME="CareCore API"

# Database
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USER=carecore
DB_PASSWORD=carecore_dev_password
DB_NAME=carecore_db
DB_SYNCHRONIZE=false

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRATION=24h

# FHIR
FHIR_VERSION=R4
FHIR_BASE_URL=http://localhost:3000/api/fhir

# Security
BCRYPT_ROUNDS=10

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# PgAdmin
PGADMIN_EMAIL=
PGADMIN_PASSWORD=
PGADMIN_PORT=
