# Application
NODE_ENV=production
PORT=3000
APP_NAME=CareCore API

# Database
DB_TYPE=postgres
DB_HOST=your-production-db-host
DB_PORT=5432
DB_USER=carecore_prod
DB_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
DB_NAME=carecore_db_prod
DB_SYNCHRONIZE=false

# JWT
JWT_SECRET=CHANGE_THIS_TO_SECURE_RANDOM_STRING_MIN_32_CHARS
JWT_EXPIRATION=24h

# FHIR
FHIR_VERSION=R4
FHIR_BASE_URL=https://api.carecore.example.com/api/fhir

# Security
BCRYPT_ROUNDS=12

# CORS
CORS_ORIGIN=https://app.carecore.example.com

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# PgAdmin (no necesario en producci√≥n)
PGADMIN_EMAIL=
PGADMIN_PASSWORD=
PGADMIN_PORT=
