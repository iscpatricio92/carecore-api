{
  "name": "@carecore/api",
  "version": "0.1.0",
  "description": "CareCore API - Plataforma de historial m√©dico digital con FHIR",
  "author": "Patricio",
  "maintainer": "Patricio",
  "maintainers": [
    {
      "name": "Patricio",
      "email": "isc.patricio@gmail.com"
    }
  ],
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build && sh scripts/create-main-symlink.sh",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.spec.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.spec.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "node scripts/coverage-header.js UNIT && jest --coverage --config jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "node scripts/coverage-header.js E2E && jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand",
    "test:integration": "jest --config jest.integration.js",
    "test:integration:cov": "jest --config jest.integration.js --coverage",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:cov": "npm run test:cov && echo '\\n' && npm run test:e2e:cov && echo '\\n' && npm run test:integration:cov",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/data-source.ts",
    "migration:create": "ts-node scripts/generate-migration.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/config/data-source.ts",
    "prepare": "echo 'Husky should be installed from root'",
    "github:create-tasks-phase1": "node scripts/create-github-tasks-phase1.js",
    "github:create-tasks-phase1:dry-run": "node scripts/create-github-tasks-phase1.js --dry-run",
    "github:create-tasks-phase2": "node scripts/create-github-tasks-phase2.js",
    "github:create-tasks-phase2:dry-run": "node scripts/create-github-tasks-phase2.js --dry-run",
    "github:create-tasks-phase3": "node scripts/create-github-tasks-phase3.js",
    "github:create-tasks-phase3:dry-run": "node scripts/create-github-tasks-phase3.js --dry-run",
    "docker:up": "make docker-up",
    "docker:down": "make docker-down",
    "docker:logs": "make docker-logs",
    "docker:keycloak:logs": "docker-compose logs -f keycloak",
    "docker:keycloak:restart": "docker-compose restart keycloak",
    "docker:keycloak:shell": "docker-compose exec keycloak /bin/bash",
    "encryption:verify": "ts-node scripts/verify-encryption.ts"
  },
  "dependencies": {
    "@carecore/shared": "*",
    "@keycloak/keycloak-admin-client": "^26.4.7",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-rate-limit": "^7.5.1",
    "fhir-kit-client": "^1.9.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "nestjs-pino": "^4.5.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
